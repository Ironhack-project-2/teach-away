<div class="container panels">
{{#each curso}}
    <div class="container area">
        <h5><b>Nombre: </b>{{this.nombre}}</h5>
        <h6><b>Descripci√≥n: </b>{{this.descripcion}}</h6>
        <h6><b>Profesor: </b>{{this.profesor}}</h6>
        <h6><b>Nivel: </b>{{this.nivel}}</h6>
        {{#each this.aulas}}
            <h6><b>Nivel: </b></h6>
            {{#each this.fechas}}
